 .container{
  display: flex;
  flex-direction: column;
  align-items: center;
  width:100%;
}

 .sub-container{
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top:100rpx;
  background-color: #f5f5f5;
} 

.books-container{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  flex-wrap: wrap;
  margin-top:10rpx;
  padding: 0 90rpx;
}

.books-container book-cmp{
  margin-bottom: 30rpx;
}

.box{
  display:flex; 
  flex-direction: row;
  justify-content: center;
  align-items: center; 
  height: 34px;
  width: 700rpx;
  color: #999999;
  border-radius: 50px;
  background-color: #f5f5f5;
}

.header{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  position: fixed;
  z-index: 99;
  width:100%;
  height:100rpx;
  border-top:1px solid #f5f5f5;
  border-bottom:1px solid #f5f5f5;
  box-shadow:0 0 3px 0 #e3e3e3; 
  background-color: #ffffff;
}

.head-img{
  margin-top:40rpx;
  width:106rpx;
  height:34rpx;
}

.box image{
  margin-right:10px;
  margin-bottom:-2px;
  width:14px;
  height:14px;
}
